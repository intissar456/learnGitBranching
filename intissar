{
    long double x, y;
    printf("Calcul de moyenne\n");
    printf("Entrez le premier nombre : ");
    scanf("%Lf", &x);
    printf("\n Entrez le deuxiï¿½me nombre : ");
    scanf("%Lf", &y);
